{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Collaborative contracts example, using Dredd and OAS to validate the provider API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "newman run -e ./test/localhost3000.postman_environment.json test/PactflowProductsAPI.postman_collection.json",
    "test:convert": "p2o ./test/PactflowProductsAPI.postman_collection.json -f ./oas/swagger.yml",
    "test:convert2": "node test/transform.js",
    "test:publish": "node test/publish.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "newman": "^5.2.3"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "postman2openapi": "^1.0.0-beta",
    "postman-to-openapi": "^1.8.0"
  },
  "license": "ISC"
}
